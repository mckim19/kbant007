{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": ["react-jsx", "react"],
    "lib": [
      "DOM",
      "DOM.Iterable",
      "esnext"
    ],
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": [
    "typing",
    "public",
    "src",
    "*.js"
  ]
}
